{
  "name": "good-home-backend",
  "version": "1.0.0",
  "description": "Real estate backend API for newly built apartments",
  "main": "src/server.js",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "node scripts/build.js",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate deploy",
    "db:push": "npx prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:backfill-i18n": "node scripts/backfill-apartment-i18n.js",
    "db:clear": "node scripts/clear-database.js",
    "db:seed-accounts": "node scripts/seed-accounts.js",
    "db:reset": "node scripts/clear-database.js && node scripts/seed-accounts.js",
    "db:reset-state": "node scripts/reset-state.js",
    "smoke:complex": "node scripts/complex-smoke.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "firebase-admin": "^13.6.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^8.0.1",
    "prisma": "^5.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
